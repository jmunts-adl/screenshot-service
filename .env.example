# Server Configuration
# Host address for the FastAPI server
HOST=0.0.0.0

# Port number for the FastAPI server
PORT=8000

# Authentication Configuration
# Bearer token for API authentication (required if API_TOKEN_REQUIRED is true)
API_TOKEN=your_api_token_here

# Whether API token authentication is required (true/false, 1/0, yes/no)
# Set to false to disable authentication (not recommended for production)
API_TOKEN_REQUIRED=true

# ScreenshotOne API Configuration
# ScreenshotOne API access key (required)
SCREENSHOTONE_ACCESS_KEY=your_screenshotone_access_key_here

# Basic proxy for ScreenshotOne API (optional)
# Used as the primary proxy for screenshot capture
SCREENSHOTONE_PROXY=your_basic_proxy_url_here

# Advanced/unlocker proxy for ScreenshotOne API (optional)
# Used as a fallback when the basic proxy fails
# This is typically more expensive, so it's only used when needed
WEB_UNLOCKER_PROXY=your_web_unlocker_proxy_url_here

# Cloudinary Configuration
# Cloudinary cloud name (required)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here

# Cloudinary API key (required)
CLOUDINARY_API_KEY=your_cloudinary_api_key_here

# Cloudinary API secret (required)
CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here

# Cloudinary folder for storing uploaded screenshots (optional)
# If not specified, screenshots will be uploaded to the root folder
CLOUDINARY_FOLDER=screenshots


ZENROWS_API_KEY=your_zenrows_api_key_here